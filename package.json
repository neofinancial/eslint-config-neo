{
  "name": "eslint-config-neo",
  "version": "0.7.0",
  "description": "Official Neo Financial ESLint configuration",
  "main": "index.js",
  "author": "Neo Financial Engineering <engineering@neofinancial.com>",
  "license": "MIT",
  "homepage": "https://github.com/neofinancial/eslint-config-neo",
  "repository": {
    "type": "git",
    "url": "https://github.com/neofinancial/eslint-config-neo.git"
  },
  "files": [
    "*.js"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,yaml,yml,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,yaml,yml,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "peerDependencies": {
    "eslint": "^7.11.0 || ^8.0.0",
    "prettier": "^1.19.0 || ^2.0.0",
    "typescript": "^3.7.0 || ^4.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "husky": "^7.0.4",
    "pretty-quick": "^3.1.2"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
}
