{
  "name": "eslint-config-neo",
  "version": "1.0.0",
  "description": "Official Neo Financial ESLint configuration",
  "main": "index.js",
  "author": "Neo Financial Engineering <engineering@neofinancial.com>",
  "license": "MIT",
  "homepage": "https://github.com/neofinancial/eslint-config-neo",
  "repository": {
    "type": "git",
    "url": "https://github.com/neofinancial/eslint-config-neo.git"
  },
  "engines": {
    "npm": ">=8.0.0"
  },
  "files": [
    "*.js"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,yaml,yml,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,yaml,yml,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick"
    }
  },
  "peerDependencies": {
    "eslint": "^8.19.0",
    "prettier": "^1.19.0 || ^2.0.0",
    "typescript": "^3.7.0 || ^4.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "husky": "^4.3.0",
    "pretty-quick": "^3.1.3"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
